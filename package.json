    "@angular/router": "^5.2.9",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^9.6.0",
    "codelyzer": "~4.2.1",
    "copyfiles": "^2.0.0",
    "gh-pages": "^1.1.0",
